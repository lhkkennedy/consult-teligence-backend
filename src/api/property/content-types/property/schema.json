{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "property_uid": {
      "type": "string",
      "unique": true
    },
    "title": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "property_type": {
      "type": "enumeration",
      "enum": ["Industrial", "Office", "Retail", "Residential"]
    },
    "status": {
      "type": "enumeration",
      "enum": ["Stabilised", "Under Construction", "Exited", "Planning"]
    },
    "headline_metric": {
      "type": "string"
    },
    "media_urls": {
      "type": "json"
    },
    "roles": {
      "type": "jsonb"
    },
    "deal_size": {
      "type": "decimal"
    },
    "irr": {
      "type": "decimal"
    },
    "completion_percentage": {
      "type": "integer"
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::consultant.consultant"
    },
    "tags": {
      "type": "json"
    }
  }
} 