{
  "kind": "collectionType",
  "collectionName": "timeline_items",
  "info": {
    "singularName": "timeline-item",
    "pluralName": "timeline-items",
    "displayName": "Timeline Item"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "post_id": {
      "type": "string",
      "unique": true
    },
    "created_at": {
      "type": "datetime"
    },
    "body_md": {
      "type": "text"
    },
    "media_urls": {
      "type": "json"
    },
    "post_type": {
      "type": "enumeration",
      "enum": ["NewListing", "ProgressUpdate", "Insight", "Closing"]
    },
    "sentiment": {
      "type": "enumeration",
      "enum": ["Bull", "Neutral", "Bear"]
    },
    "visibility": {
      "type": "enumeration",
      "enum": ["Public", "Private", "ProfileSpecific"]
    },
    "reactions": {
      "type": "json"
    },
    "comments": {
      "type": "json"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::consultant.consultant"
    },
    "property": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::property.property"
    },
    "property_uid": {
      "type": "string"
    }
  }
} 